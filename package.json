    "extract-zip": "^2.0.1",
    "fast-glob": "^3.2.4",
    "node-fetch": "^2.6.1",
    "ora": "^5.1.0",
    "prompts": "^2.4.0"
  },
  "devDependencies": {
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2"
  }
}
